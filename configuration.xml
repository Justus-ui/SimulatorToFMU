<?xml version="1.0" encoding="UTF-8"?>
<SimulatorModelDescription
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  fmiVersion="2.0"
  modelName="abc"
  description="Input data for a Simulator FMU"
  generationTool="SimulatorToFMU">
  <ModelVariables>
    <ScalarVariable
      name="_configurationFileName"
      description="parameter"
      causality="parameter"
	  start="C:/Users/Public/server_config.txt"
	  type="String">
    </ScalarVariable>
    <ScalarVariable
      name="x"
      description="Voltage"
      causality="input"
	  type="Real"
	  unit="V"
	  start="0.0">
    </ScalarVariable>
    <ScalarVariable
      name="y"
      description="Current"
      causality="output"
	  type="Real"
	  unit="V">
    </ScalarVariable>
  </ModelVariables>
</SimulatorModelDescription>